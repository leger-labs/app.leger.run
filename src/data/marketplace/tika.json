{
  "id": "tika",
  "name": "Apache Tika",
  "category": "content-extraction",
  "requires_api_key": false,
  "logo": "@/assets/icons/tika.svg",
  "openwebui_variables": [
    {
      "name": "TIKA_SERVER_URL",
      "type": "str",
      "default": "http://localhost:9998",
      "description": "Sets the URL for the Apache Tika server.",
      "options": []
    },
    {
      "name": "TIKA_HEAP_SIZE",
      "type": "str",
      "default": "2g",
      "description": "Tika heap size",
      "options": []
    }
  ]
}
