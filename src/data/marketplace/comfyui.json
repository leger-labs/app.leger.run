{
  "id": "comfyui",
  "name": "ComfyUI",
  "category": "image-generation",
  "requires_api_key": false,
  "logo": "@/assets/icons/comfyui.svg",
  "openwebui_variables": [
    {
      "name": "COMFYUI_API_KEY",
      "type": "str",
      "default": null,
      "description": "Sets the API key for ComfyUI.",
      "options": []
    },
    {
      "name": "COMFYUI_BASE_URL",
      "type": "str",
      "default": null,
      "description": "Specifies the URL to the ComfyUI image generation API.",
      "options": []
    },
    {
      "name": "COMFYUI_WORKFLOW",
      "type": "str",
      "default": "",
      "description": "Sets the ComfyUI workflow.",
      "options": []
    },
    {
      "name": "COMFYUI_WORKFLOW_NODES",
      "type": "list",
      "default": [],
      "description": "Specifies the ComfyUI workflow node mappings for image generation.",
      "options": []
    },
    {
      "name": "IMAGE_GENERATION_ENGINE",
      "type": "str",
      "default": "openai",
      "description": "Specifies the engine to use for image generation.",
      "options": [
        "openai",
        "comfyui",
        "automatic1111",
        "gemini"
      ]
    },
    {
      "name": "COMFYUI_WORKSPACE",
      "type": "str",
      "default": "/workspace",
      "description": "ComfyUI workspace path",
      "options": []
    },
    {
      "name": "COMFYUI_AUTO_UPDATE",
      "type": "bool",
      "default": false,
      "description": "ComfyUI auto-update flag",
      "options": []
    },
    {
      "name": "COMFYUI_WEB_ENABLE_AUTH",
      "type": "bool",
      "default": false,
      "description": "ComfyUI web authentication",
      "options": []
    },
    {
      "name": "COMFYUI_LOG_LEVEL",
      "type": "str",
      "default": "INFO",
      "description": "ComfyUI log level",
      "options": [
        "DEBUG",
        "INFO",
        "WARNING",
        "ERROR"
      ]
    },
    {
      "name": "COMFYUI_ARGS",
      "type": "str",
      "default": "",
      "description": "ComfyUI additional arguments",
      "options": []
    },
    {
      "name": "COMFYUI_STARTUP_TIMEOUT",
      "type": "int",
      "default": 900,
      "description": "ComfyUI startup timeout in seconds",
      "validation": {
        "min": 60,
        "max": 1800
      },
      "options": []
    }
  ]
}
